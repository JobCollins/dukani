    "url": "https://github.com/JobCollins/dukani/issues"
  },
  "homepage": "https://github.com/JobCollins/dukani#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
