{
  "name": "dukani",
  "version": "1.0.0",
  "description": "Dukani is a MERN ecommerce application. Dukani is a Swahili word meaning at the shop.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
